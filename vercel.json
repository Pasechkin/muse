{
  "version": 2,
  "buildCommand": "npm install && npm run build:once && npm run copy-css",
  "outputDirectory": "src/html",
  "installCommand": "npm install",
  "devCommand": "npm run build",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

